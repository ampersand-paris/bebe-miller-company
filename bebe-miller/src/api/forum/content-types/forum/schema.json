    "pluralName": "forums",
    "displayName": "Forum",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Forum_Title": {
      "type": "string"
    },
    "Forum_Category": {
      "type": "string"
    },
    "Date_Published": {
      "type": "date"
    },
